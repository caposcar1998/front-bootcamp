{
    "request": {
      "urlPathPattern": "/api/v1/users",
      "method": "PUT",
      "queryParameters": {
        "id": {
          "equalTo": "1"
        },
      "bodyPatterns" : [{
        "matchesJsonPath" : {
          "expression": "$",
          "equalToJson": "{ 'FirstName':'Oscar', 'LastName':'Contreras', 'Email':'oscar@oscar.co', 'Password': 123456}"
          }
        }]
    },
    "response": {
      "status": 200,
      "body": "{ \"res\": \"ok\" }",
      "headers": {
        "Content-Type": "application/json"
      }
    }
}



